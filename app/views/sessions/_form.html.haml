%br

.row
  .col.s6.black-text
    %h3 Login to Your Account
    %br
    = form_for(:session, url: login_path) do |f|
      .input-field
        = f.label :username
        = f.text_field :username
      %br
      .input-field
        = f.label :password
        = f.password_field :password
      %br
      %br
      .waves-effect.waves-dark.btn
        = f.submit "Login"

%br
%br
